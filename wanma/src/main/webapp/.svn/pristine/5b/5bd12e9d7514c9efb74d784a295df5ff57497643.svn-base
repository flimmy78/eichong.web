/*端电源模块==========================*/
.MyBox {
	width: 88%;
	background: #333;
	margin-top: 50px;
	/*
	display: flex;
	display: -webkit-flex;
	flex-direction: row;
	flex-wrap: wrap;
	justify-content: flex-start;*/
	font-size: 14px;
	overflow: hidden;
}

.MyBox .BoxLeft {
	width: 240px;
	height: 350px;
	background: #5C5C5C;
	border-radius: 10px;
	margin: 10px 20px 10px;
	padding: 20px;
	float:left;
}

.powerModule {
	width: 930px;
	min-height: 380px;
	background: #32B16C;
	position: absolute;
	display: none;
	border-radius: 8px;
}

.powerModule .powerModuleTip {
	width: 240px;
	height: 26px;
	background: #5C5C5C;
	border-radius: 8px;
	padding: 15px 20px;
	margin-left: 20px;
	margin-top: 20px;
}

.powerModule .powerModuleTip .powerModuleBlock {
	width: 100%;
	height: 100%;
	line-height: 26px;
	color: #FFFFFF;
}

.powerModule .powerModuleList {
	width: 900px;
	height: 278px;
	padding: 0;
	list-style: none;
	margin-top: 20px;
	margin-left: 20px;
	/*
	display: flex;
	display: -webkit-flex;
	flex-direction: row;
	flex-wrap: wrap;*/
}

.powerModule .powerModuleList li {
	width: 280px;
	height: 278px;
	border-radius: 8px;
	background: #5C5C5C;
	padding: 0;
	margin-left: 20px;
}

.MyBox .BoxLeft .powerModuleBlock {
	width: 90%;
	height: 36px;
	background: #333333;
	line-height: 36px;
	color: #FFFFFF;
	padding-left: 20px;
	position: relative;
}

.MyBox .BoxLeft .powerModuleBlock .line {
	display: block;
	cursor: pointer;
	width: 24px;
	height: 24px;
	background: url(/wanma/static/images/img/pileDetail/line.png) no-repeat
		center;
	position: absolute;
	right: 5px;
	top: 4px;
}

.MyBox .BoxLeft .powerModuleStatus {
	width: 90%;
	height: 30px;
	line-height: 30px;
	padding-top: 10px;
	color: #FFFFFF;
	padding-left: 20px;
}

.MyBox .BoxLeft .powerModuleStatus .powerModuleStatus-use {
	display: inline-block;
	width: 60px;
	height: 30px;
	text-align: center;
    line-height: 30px;
	background: #32b16c;
	border-radius: 4px;
}

.MyBox .BoxLeft .powerModuleDataList {
	padding: 0;
	margin: 0;
	list-style: none;
	border-bottom: 2px solid #333333;
	color: #FFFFFF;
	padding: 20px 60px 20px 20px;
}

.MyBox .BoxLeft .powerModuleDataList li {
	width: 100%;
	height: 24px;
}

.MyBox .BoxLeft .powerModuleDataList li span {
	float: right;
	color: #f6972d;
}

.fanye_box {
	float: left;
	height: 100%;
	padding: 0;
}

.fanye_box li {
	list-style: none;
	width: 24px;
	height: 10px;
	float: left;
	margin-top: 0%;
	margin-right: 10px;
	text-align: center;
	color: #ccc;
}
/***分页***/
.fanye_box li a {
	color: #5d5d5d;
	text-decoration: none;
}

.fanye_box li a:hover {
	color: #000;
	text-decoration: none;
}

.fanye_box li.dangqian a {
	font-size: 16px;
	font-weight: bold;
	color: #ff8800;
}

.fanye_box li.qianfan, .fanye_box li.houfan {
	height: 12px;
	font-size: 10px;
	color: #ccc;
	background: #898989;
	width: 12px;
}

.fanye_box li.qianfan:hover, .fanye_box li.houfan:hover {
	font-size: 10px;
	color: #fff;
	background: #5d5d5d;
}

.fanye_box li.qianfan a, .fanye_box li.houfan a {
	color: #fff;
	text-decoration: none;
}

